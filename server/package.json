{
  "name": "momo-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "pm2": "cross-env NODE_ENV=production pm2 start ./src/server.ts --watch",
    "start": "cross-env NODE_ENV=production ts-node ./src/server.ts",
    "reset:prod": "cross-env NODE_ENV=production DATABASE_INIT=true ts-node ./src/server.ts",
    "reset:dev": "cross-env NODE_ENV=development DATABASE_INIT=true ts-node ./src/server.ts",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node ./src/server.ts",
    "test": "cross-env NODE_ENV=test jest --runInBand --verbose --detectOpenHandles --forceExit --testTimeout=30000 ./tests/*.test.ts exit",
    "test:watch": "cross-env NODE_ENV=test jest --runInBand --verbose --detectOpenHandles --watchAll --testTimeout=30000 ./tests/*.test.ts",
    "test:coverage": "cross-env NODE_ENV=test jest --runInBand --verbose --detectOpenHandles --forceExit --coverage --testTimeout=30000 ./tests/*.test.ts exit",
    "build": "tsc -p .",
    "postbuild": "swagger-cli bundle ./src/docs/openapi.yaml --outfile build/docs/openapi.yaml --type yaml"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.16.4",
    "google-auth-library": "^6.1.3",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.9.1",
    "rand-token": "^1.0.1",
    "sequelize": "^5.22.0",
    "sequelize-cli": "^6.2.0",
    "swagger-cli": "^4.0.4",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6",
    "verify-apple-id-token": "^2.0.3",
    "yamljs": "^0.3.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.9",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.20",
    "@types/json2csv": "^5.0.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.16",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
